<!-- Preguntas pelo -->
<category>
<pattern>* <set>pelo</set> ^</pattern>
<template>
	<think>
		<set var="existe">
			<srai>FINDITEM <star index="2"/> IN <get name="Lista"/></srai>
		</set>
	</think>
	<condition var="existe">
		<li value="0">
			No, no es de pelo <star index="2"/>
		</li>
		<li>
			Si, es de pelo <star index="2"/>
		</li>
	</condition>
</template>
</category>


